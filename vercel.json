{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/old-route",
      "status": 301,
      "dest": "/new-route"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": "https://api.miltonprotocol.com"
  }
}
